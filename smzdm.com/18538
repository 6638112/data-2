<a href="https://go.smzdm.com/7bb9d5540de3a535/ca_bb_yc_37_71109253_10111_39235_45_0" onclick="sensors.track('AddToCart', {'article_title':'现代木工入门 做个简洁实用的菜刀架','article_id':'a83go066','brand':'Dongcheng/东成','mall_name':'京东','channel':'yuanchuang','go_path':'https://go.smzdm.com/7bb9d5540de3a535/ca_bb_yc_37_71109253_10111_39235_45_0','channel_id':'11','cate_level1':'家居家装'});gtmAddToCart({'name':'东成电动工具附件硬质合金木工开孔器木工钻头扩孔工具16-35mm16mm','id':'a83go066' ,'dimension16':'','dimension19':'2130706433160160657969435265bf93488c5bf4fceafea6a0c13401e1', 'price':'8.8','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'8.8','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'1阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">东成电动工具附件硬质合金木工开孔器木工钻头扩孔工具16-35mm16mm</span></a>
