<a href="https://go.smzdm.com/ad4c36a70580047c/ca_bb_yc_27_71231410_13107_249_29_0" onclick="sensors.track('AddToCart', {'article_title':'OLED电视篇：PS5、XSX到手了？双十二如何挑选第一台次时代游戏电视','article_id':'aqn2werx','brand':'SONY/索尼','mall_name':'无','channel':'yuanchuang','go_path':'https://go.smzdm.com/ad4c36a70580047c/ca_bb_yc_27_71231410_13107_249_29_0','channel_id':'11','cate_level1':'家用电器'});gtmAddToCart({'name':'LGOLED65CXPCA65英寸OLED电视4K高清HDR自发光全面屏液晶智能平板电视客厅推荐强劲游戏性能HDMI2.1','id':'aqn2werx' ,'dimension16':'','dimension19':'58692681316073233819096314d8b305bf98fa43169b70f796104cd17', 'price':'17999','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'17999','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'10阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">LG 乐金 OLED65C9PCA 65英寸 超高清4K 电视  </span></a>
