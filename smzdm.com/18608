<a href="https://go.smzdm.com/7b1f5cf8a7f6ebb0/ca_bb_yc_163_71245722_10111_249_175_0" onclick="sensors.track('AddToCart', {'article_title':'降噪耳机选购指南：六款耳机神仙打架？降噪耳机闭眼买清单','article_id':'apz0n9v9','brand':'SONY/索尼','mall_name':'京东','channel':'yuanchuang','go_path':'https://go.smzdm.com/7b1f5cf8a7f6ebb0/ca_bb_yc_163_71245722_10111_249_175_0','channel_id':'11','cate_level1':'电脑数码'});gtmAddToCart({'name':'漫步者（EDIFIER）FunBuds真无线降噪耳机主动降噪蓝牙耳机降噪耳机通用苹果安卓手机雪映白','id':'apz0n9v9' ,'dimension16':'','dimension19':'30623140591589615147152518816d451f2e271fec9ed8aecf792cc73c', 'price':'399','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'399','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'5阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" >
<span class="card-btn-deep">去购买</span>
