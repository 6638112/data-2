<a href="https://go.smzdm.com/aa0fefe3868b8ea4/ca_bb_yc_27_71020476_12751_36691_31_0" onclick="gtmAddToCart({'name':'全新加湿器雾化片/卡通换能片/超声波加湿片/20mm/20MM/通用型','id':'alp6vlx8' ,'dimension16':'','dimension19':'', 'price':'6.65','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'6.65','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'1阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">全新加湿器雾化片/卡通换能片/超声波加湿片/20mm/20MM/通用型</span></a>
