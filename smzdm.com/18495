<a href="https://go.smzdm.com/aade70dfa50a2531/ca_bb_yc_177_71005863_10111_1613_189_0" onclick="gtmAddToCart({'name':'永生钢笔学生用练字成人书写办公正姿特细墨囊墨水商务儿童男孩女孩送礼卡通美工','id':'andrzxwv' ,'dimension16':'','dimension19':'3708445987155537552073445314d7890a8af5eee5b5dbf41cc7d71cd0', 'price':'7.8','brand':'无' ,'mall':'天猫精选', 'category':'无/无/无/无','metric1':'7.8','dimension10':'tmall.com','dimension9':'yuanchuang','dimension11':'1阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">永生 1103 学生钢笔 2支装</span></a>
