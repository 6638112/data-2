<a href="https://go.smzdm.com/71582c20296f06d7/ca_bb_yc_163_71108097_10869_1933_165_0" onclick="sensors.track('AddToCart', {'article_title':'小米对讲机Lite “黑白双煞”的再度“合体”','article_id':'ag8lx9wd','brand':'MI/小米','mall_name':'无','channel':'yuanchuang','go_path':'https://go.smzdm.com/71582c20296f06d7/ca_bb_yc_163_71108097_10869_1933_165_0','channel_id':'11','cate_level1':'电脑数码'});gtmAddToCart({'name':'小米对讲机Lite充电座','id':'ag8lx9wd' ,'dimension16':'','dimension19':'', 'price':'39','brand':'无' ,'mall':'小米官网', 'category':'无/无/无/无','metric1':'39','dimension10':'mi.com','dimension9':'yuanchuang','dimension11':'2阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">小米对讲机Lite充电座</span></a>
