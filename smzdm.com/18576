<span class="embed-card-info">
<a href="https://go.smzdm.com/f2fa352d6eb486f2/ca_bb_yc_7_71176049_12607_0_1643_0" onclick="sensors.track('AddToCart', {'article_title':'绘本里的沧海遗珠 | 编辑们的压箱底书单 ①','article_id':'ag8l5353','brand':'无','mall_name':'无','channel':'yuanchuang','go_path':'https://go.smzdm.com/f2fa352d6eb486f2/ca_bb_yc_7_71176049_12607_0_1643_0','channel_id':'11','cate_level1':'图书杂志'});gtmAddToCart({'name':'《竹林里的青蛙公主》','id':'ag8l5353' ,'dimension16':'','dimension19':'20619672051588488239248995f50e71a36466305e5a6740fb371c6eb5', 'price':'52.5','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'52.5','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'2阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">竹林里的青蛙公主</span></a>
