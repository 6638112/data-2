<a href="https://go.smzdm.com/019e59eb2c2e7c88/ca_bb_yc_7_71075732_213_0_1643_0" onclick="gtmAddToCart({'name':'我的二本学生','id':'av7o2x7m' ,'dimension16':'','dimension19':'2130706433159987840293436664ddb0eced79ac15b750556cf5c853f6', 'price':'34.9','brand':'无' ,'mall':'当当', 'category':'无/无/无/无','metric1':'34.9','dimension10':'dangdang.com','dimension9':'yuanchuang','dimension11':'2阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">我的二本学生</span></a>
