<a href="https://go.smzdm.com/5a8554f95d904804/ca_bb_yc_163_71045900_10289_1461_179_0" onclick="gtmAddToCart({'name':'墨一微软surface触控笔Laptop/Go2触屏笔Pro7/6/5/4电容笔Book平板手写笔4096级倾斜压感【5代玉石黑】','id':'a5k45npl' ,'dimension16':'','dimension19':'21307064331599455753358608647355004181bd9869eb974be69dfc3a', 'price':'258','brand':'无' ,'mall':'京东', 'category':'无/无/无/无','metric1':'258','dimension10':'jd.com','dimension9':'yuanchuang','dimension11':'5阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" >
