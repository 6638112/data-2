<div class="coupon-card-wraper" data-type="站外商品卡片">
<span class="embed-card main-site coupon-card">
<span class="z-tag-baike">唯品会</span>
<span class="embed-card-logo">
<a href="https://go.smzdm.com/5776920236f82cca/ca_bb_yc_163_71220156_11335_249_175_0" onclick="sensors.track('AddToCart', {'article_title':'30-1300元值得买的6款有线/无线蓝牙耳机','article_id':'av7ogo5p','brand':'SONY/索尼','mall_name':'无','channel':'yuanchuang','go_path':'https://go.smzdm.com/5776920236f82cca/ca_bb_yc_163_71220156_11335_249_175_0','channel_id':'11','cate_level1':'电脑数码'});gtmAddToCart({'name':'Apple原装EarPodsLighting口苹果耳机','id':'av7ogo5p' ,'dimension16':'','dimension19':'', 'price':'147','brand':'无' ,'mall':'唯品会', 'category':'无/无/无/无','metric1':'147','dimension10':'vip.com','dimension9':'yuanchuang','dimension11':'4阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" >
