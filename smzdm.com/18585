<a href="https://go.smzdm.com/f70235e884dd9eb6/ca_bb_yc_1515_71192636_11386_61587_47_0" onclick="sensors.track('AddToCart', {'article_title':'我的双十一剁手战绩：康巴赫（KBH）316L不锈钢煎炒两用锅使用体验','article_id':'az5g7gk0','brand':'KBH/康巴赫','mall_name':'无','channel':'yuanchuang','go_path':'https://go.smzdm.com/f70235e884dd9eb6/ca_bb_yc_1515_71192636_11386_61587_47_0','channel_id':'11','cate_level1':'日用百货'});gtmAddToCart({'name':'康巴赫官方旗舰316不锈钢煎炒锅家用不粘锅炒菜锅燃气灶电磁炉','id':'az5g7gk0' ,'dimension16':'','dimension19':'', 'price':'399','brand':'无' ,'mall':'天猫精选', 'category':'无/无/无/无','metric1':'399','dimension10':'tmall.com','dimension9':'yuanchuang','dimension11':'5阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" ><span class="card-name">康巴赫官方旗舰316不锈钢煎炒锅家用不粘锅炒菜锅燃气灶电磁炉</span></a>
