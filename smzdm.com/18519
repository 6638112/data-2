<a href="https://go.smzdm.com/7153bb45272c21a5/ca_bb_yc_177_71067957_13107_623_189_0" onclick="gtmAddToCart({'name':'郡士君士高达模型上色油性马克笔GM消色笔上色笔勾线渗线笔','id':'ammqwk5k' ,'dimension16':'','dimension19':'', 'price':'12.9','brand':'无' ,'mall':'天猫精选', 'category':'无/无/无/无','metric1':'12.9','dimension10':'tmall.com','dimension9':'yuanchuang','dimension11':'1阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" >
<span class="card-btn-deep">去购买</span>
