var yname = ["摄氏度"]; //Y轴1单位，Y轴2单位(如果有的话)
var leftside = "8%"; //左边空白
var yax = 0; //是否双Y轴
var yminmax = [20, 90]; //Y轴最小值最大值，默认全0为自动，全1为自动以最小值数据最小值
var fill = 0; //0-1表示填充透明度
var sn = 5; //坐标轴分割为多少段
